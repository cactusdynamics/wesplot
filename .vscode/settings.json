{
  "chat.customAgentInSubagent.enabled": true,
  "chat.agent.maxRequests": 300,
  "chat.tools.terminal.autoApprove": {
    "/^make test$/": {
      "approve": true,
      "matchCommandLine": true
    },
    "/^make test COVERAGE=1$/": {
      "approve": true,
      "matchCommandLine": true
    },
    "/^go test [.]\/[.][.][.]$/": {
      "approve": true,
      "matchCommandLine": true
    },
    "/^make lint$/": {
      "approve": true,
      "matchCommandLine": true
    },
    "/^make prod$/": {
      "approve": true,
      "matchCommandLine": true
    },
    "/^cd frontend$/": {
      "approve": true,
      "matchCommandLine": true
    },
    "/^npm run test$/": {
      "approve": true,
      "matchCommandLine": true
    },
    "/^npm run benchmark$/": {
      "approve": true,
      "matchCommandLine": true
    },
    "/^npm run lint$/": {
      "approve": true,
      "matchCommandLine": true
    },
  },
  "biome.lsp.bin": "./frontend/node_modules/@biomejs/biome/bin/biome",
  "biome.configurationPath": "./frontend/biome.json"
}
